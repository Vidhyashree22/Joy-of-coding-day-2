#include <stdio.h>

int main()
{
    int a;
    printf("Enter the age:");
    scanf("%d",&a);
    if(a<18)
    {
        printf("Not eligible");
    }
    else
    {
        printf("Eligible");
    }
    return 0;
}







#include <stdio.h>

int main()
{
    int age,num;
    printf("Enter the age:");
    scanf("%d",&age);
    num=age>=18?1:0;
    if(num==1)
    {
        printf("Eligible");
    }
    else
    {
        printf("Not eligible");
    }
    return 0;
}


